<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MainBackgroundBrush" Color="White"/>
    <Color x:Key="MainBackgroundColor" A="255" R="25" G="25" B="25"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="MainDialogBottomBrush" Color="#F0F0F0"/>
    <Color x:Key="BtnBackgroundColor" A="255" R="221" G="221" B="221"/>
    <Color x:Key="BtnHoverColor" A="255" R="190" G="230" B="253"/>
    <SolidColorBrush x:Key="TextBrush" Color="Black"/>
    <SolidColorBrush x:Key="TabControlBrush" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="TabSelectedBrush" Color="#FF006ABC"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="White"/>
    <Color x:Key="HoverColor" A="255" R="190" G="230" B="253"/>
    <SolidColorBrush x:Key="HoverBrush" Color="{DynamicResource HoverColor}"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#FFBBBBBB"/>
    <Color x:Key="ComboBoxBGBrush" A="255" R="225" G="225" B="225"/>
</ResourceDictionary>