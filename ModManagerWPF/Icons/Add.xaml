<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<DrawingImage x:Key="IconAdd">
		<DrawingImage.Drawing>
			<DrawingGroup ClipGeometry="M0,0 V24 H24 V0 H0 Z">
				<GeometryDrawing Geometry="F1 M24,24z M0,0z M12,6L12,18 M18,12L6,12">
					<GeometryDrawing.Pen>
						<Pen Brush="{StaticResource IconBrushAdd}" Thickness="2" StartLineCap="Round" EndLineCap="Round" LineJoin="Round" MiterLimit="1" />
					</GeometryDrawing.Pen>
				</GeometryDrawing>
			</DrawingGroup>
		</DrawingImage.Drawing>
	</DrawingImage>
</ResourceDictionary>